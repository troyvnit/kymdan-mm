        function showComment(e) {
            e.preventDefault();
            var dataItem = this.dataItem($(e.currentTarget).closest("tr"));
            if (dataItem.Id == 0) {
                alert("Phải lưu trước khi phản hồi!");
                return false;
            }
            commentWindow = $("#CommentWindow")
                .kendoWindow({
                    title: "Phản hồi",
